{
  "name": "christoph-kluge/bref-sqs-laravel",
  "description": "Laravel adapter for bref",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Christoph Kluge",
      "email": "work@christoph-kluge.eu"
    }
  ],
  "require": {
    "bref/bref": "^0.5"
  },
  "require-dev": {
    "laravel/framework": "^5.8 || ^6.0"
  },
  "autoload": {
    "psr-4": {
